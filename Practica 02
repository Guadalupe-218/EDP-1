        "from math import exp\n",
        "from sympy import fourier_series, pi, plot, Piecewise, exp\n",
        "from sympy.abc import x\n",
        "\n",
        "f=Piecewise(\n",
        "    (-exp(-2*x), (-1 <= x) & ( x < 0)),\n",
        "    (exp(2*x), (0 <= x) & ( x <= 1)))\n",
        "\n",
        "\n",
        "\n",
        "plot(f, (x,0,1),line_color ='red')\n",
        "s = fourier_series(f, (x, 0, 1))\n",
        "s1 = s.truncate(n = 3)\n",
        "s2 = s.truncate(n = 10)\n",
        "s3 = s.truncate(n = 7)\n",
        "p = plot(f, s1, s2, s3, (x, -1, 1), show=False, legend=True)"
